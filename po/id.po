msgstr "Kata Sandi"

#. This error is shown when authentication fails.
#: src/app/components/login/login.ui:156
msgid "Authentication failed!"
msgstr "Autentikasi gagal!"

#. Log in button label
#: src/app/components/login/login.ui:181
msgid "Log in"
msgstr "Log masuk"

